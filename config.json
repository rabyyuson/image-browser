{
    "navigation": [
        {
            "url": "/",
            "label": "Collections"
        },
        {
            "url": "/list",
            "label": "List"
        }
    ],
    "unsplash_api_endpoint": "https://api.unsplash.com",
    "unsplash_api": {
        "photos" : {
            "collections": {
                "endpoint": "/collections",
                "per_page": "30"
            },
            "list": {
                "endpoint": "/photos",
                "per_page": "30",
                "order_by": [ "latest", "popular", "oldest" ]
            },
            "search": {
                "endpoint": "/search/photos",
                "per_page": "30",
                "order_by": [ "relevant", "latest" ]
            }
        }
    }
}